{
    "workbench.startupEditor": "none",
    "terminal.integrated.rendererType": "dom",
    "editor.minimap.enabled": false,
    "editor.tabSize": 2,
    "editor.renderWhitespace": "boundary",
    "editor.wordWrap": "on",
    "editor.cursorBlinking": "solid",
    "editor.fontWeight": "500",
    "files.trimTrailingWhitespace": true,
    "editor.hover.delay": 200,
    "workbench.fontAliasing": "auto",
    "workbench.statusBar.feedback.visible": false,
    "window.restoreFullscreen": true,
    "terminal.external.osxExec": "iTerm.app",
    "terminal.integrated.enableBell": true,
    "css.lint.boxModel": "warning",
    "css.lint.compatibleVendorPrefixes": "warning",
    "css.lint.duplicateProperties": "error",
    "css.lint.emptyRules": "error",
    "css.lint.float": "warning",
    "css.lint.idSelector": "warning",
    "css.lint.important": "warning",
    "css.lint.universalSelector": "warning",
    "css.lint.zeroUnits": "warning",
    "scss.lint.boxModel": "warning",
    "scss.lint.compatibleVendorPrefixes": "warning",
    "scss.lint.emptyRules": "error",
    "scss.lint.float": "warning",
    "scss.lint.idSelector": "warning",
    "scss.lint.important": "warning",
    "scss.lint.universalSelector": "warning",
    "eslint.alwaysShowStatus": true,
    "grunt.autoDetect": "off",
    "gulp.autoDetect": "off",
    "debug.node.autoAttach": "on",
    "vim.easymotion": true,
    "vim.hlsearch": true,
    "vim.leader": ",",
    "vim.normalModeKeyBindings": [
        {
            "before": ["space"],
            "after": ["/"]
        },
        {
            "before": ["t"],
            "commands": ["workbench.action.terminal.focus"]
        },
        {
            "before": ["g", "c"],
            "commands": "git.commit"
        },
        {
            "before": ["g", "a"],
            "commands": "git.stage"
        },
        {
            "before": ["<leader>", "w"],
            "commands": "workbench.action.files.save"
        },
        {
            "before": ["<leader>", "n"],
            "commands": "workbench.action.toggleSidebarVisibility"
        },
        {
            "before": ["<C-k>"],
            "commands": "workbench.action.nextEditor"
        },
        {
            "before": ["<C-j>"],
            "commands": "workbench.action.nextEditor"
        }
    ],
    "window.zoomLevel": 0,
    "editor.scrollBeyondLastLine": false,
    "emmet.triggerExpansionOnTab": true,
    "editor.suggest.localityBonus": true,
    "editor.suggestSelection": "recentlyUsedByPrefix",
    "workbench.tree.horizontalScrolling": true,
    "update.showReleaseNotes": false,
    "liveshare.featureSet": "insiders",
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "terminal.integrated.shell.osx": "/bin/zsh",
    "gitlens.codeLens.enabled": false,
    "gitlens.hovers.currentLine.over": "line",
    "ruby.rubocop.executePath": "/Users/shuyoshioka/.rbenv/shims/",
    "ruby.rubocop.useBundler": true,
    "prettier.singleQuote": true,
    "prettier.requireConfig": true,
    "workbench.colorTheme": "Material Theme Darker",
    "vetur.format.defaultFormatter.js": "prettier-eslint",
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "diffEditor.ignoreTrimWhitespace": false,
    "sync.gist": "0ce7401386690d5e1f73c55c2b555092"
}